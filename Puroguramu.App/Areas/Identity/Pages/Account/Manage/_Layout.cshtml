@{
    if (ViewData.TryGetValue("ParentLayout", out var parentLayout))
    {
        Layout = (string)parentLayout!;
    }
    else
    {
        Layout = "Shared/_Layout";
    }
}



<div>

        <div class="col-md-9">
            @RenderBody()
        </div>
   
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
